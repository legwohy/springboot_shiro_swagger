CREATE TABLE `user_info_from` (
  `id` int(18) NOT NULL AUTO_INCREMENT,
  `real_name` varchar(255) DEFAULT NULL COMMENT '真实姓名',
  `user_phone` varchar(11) DEFAULT NULL,
  `id_card_number` varchar(18) DEFAULT NULL COMMENT '身份证号码',
  `idcard_img_z` varchar(200) DEFAULT '' COMMENT '身份证正面',
  `idcard_img_f` varchar(200) DEFAULT '' COMMENT '身份证反面',
  `idcard_img_s` varchar(200) DEFAULT '' COMMENT '手持身份证',
  `work_address` varchar(200) DEFAULT NULL COMMENT '工作地址',
  `first_contact_name` varchar(255) DEFAULT '' COMMENT '第一联系人姓名',
  `first_contact_phone` varchar(255) DEFAULT '' COMMENT '第一联系人电话',
  `first_contact_relation` int(2) DEFAULT NULL COMMENT '与第一联系人的关系(1父亲、2母亲、3儿子、4女儿、5配偶、6兄弟、7姐妹)',
  `second_contact_name` varchar(255) DEFAULT NULL COMMENT '第二联系人姓名',
  `second_contact_phone` varchar(255) DEFAULT NULL COMMENT '第二联系人电话',
  `second_contact_relation` int(2) DEFAULT NULL COMMENT '与第二联系人的关系(1.同学2.亲戚3.同事4.朋友5.其他)',
  `channel_id` int(10) DEFAULT NULL COMMENT '渠道id',
  `merchant_id` int(10) NOT NULL COMMENT '所属商户id（back_user表中的id）',
  `status` int(10) DEFAULT '1' COMMENT '1 待审核，2 审核通过，3 待放款(待定)，4已放款(还款中)(待定)，5已拒绝，6已取消(待定)，7已结清(待定)，8已逾期(待定)，9失联(待定)',
  `update_status_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '用户状态跟新时的时间',
  `create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `update_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '跟新时间',
  `real_name_status` int(2) DEFAULT '0' COMMENT '0未实名认证  1已实名认证    2  实名认证失败',
  `update_contact_count` int(2) DEFAULT '0' COMMENT '联系人修改过的次数',
  `contact_status` int(2) DEFAULT '0' COMMENT '联系人认证状态（0:未完成  1:已完成[可修改]  2:已完成[不可修改]）',
  `risk_status` int(2) DEFAULT '0' COMMENT '资料填写完成 1 可以进行风控采集 2 已完成采集',
  `empower_status` int(2) DEFAULT '0' COMMENT '借条授权状态（0：还没有授权过  1：已授权 ',
  `jxl_token` varchar(50) DEFAULT NULL COMMENT '聚信立开始采集数据时存入token',
  `jxl_token_time` timestamp NULL DEFAULT NULL COMMENT '聚信立token入库时间',
  `jxl_status` int(2) DEFAULT '0' COMMENT '0未认证  1已认证 ',
  `jxl_detail_time` timestamp NULL DEFAULT NULL COMMENT '聚信立成功采集详情数据的时间',
  `jxl_detail_status` int(2) DEFAULT '0' COMMENT '0.聚信立无报告，1报告已入库，写入detail的时候写入这个值',
  `assessor_work_info_id` varchar(50) DEFAULT NULL COMMENT '审核员的id',
  `qq_contact_status` int(11) DEFAULT '0' COMMENT 'qq联系人拉取状态',
  `risk_rules_merchant_status` int(2) DEFAULT '0' COMMENT '商户规则 0 初始  1 通过 2 拒绝',
  PRIMARY KEY (`id`),
  UNIQUE KEY `index_m_u` (`merchant_id`,`user_phone`) USING BTREE COMMENT '商户和手机号码唯一索引'
) ENGINE=InnoDB AUTO_INCREMENT=814 DEFAULT CHARSET=utf8 COMMENT='H5推广注册用户';

